<%= form_for(@ad_set) do |f| %>
  <% if @ad_set.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@ad_set.errors.count, "error") %> prohibited this ad_set from being saved:</h2>

      <ul>
      <% @ad_set.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :video_id %><br />
    <%= select("ad_set","video_id",Video.all.collect{|v| [v.name,v.id]}) %>
  </div>
  <div class="field">
    <%= f.label :account_id %><br />
    <%= select("ad_set","account_id",Account.all.collect{|a| [a.company_name,a.id]}) %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
